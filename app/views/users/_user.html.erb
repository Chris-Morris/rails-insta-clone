<div class="card mt-4 rounded" style="width: 18rem;" id="<%= dom_id user %>">
  <%= link_to user, :action => 'go' do %>
    <img src="https://worldwidetravel.tips/wp-content/uploads/2021/12/Where-are-the-Maldives-Located-_120-256x256.jpg" class="card-img-top" alt="...">
  <% end %>
  <div class="card-body">
    <h5 class="card-title"><%= user.first_name %> <%= user.last_name %></h5>
    <p class="card-text"><%= user.email %></p>
  </div>
</div>